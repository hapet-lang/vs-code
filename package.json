{
  "name": "hapetvscode",
  "displayName": "HapetVsCode",
  "description": "Hapet language for Visual Studio Code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "hapet",
      "aliases": ["Hapet", "hapet"],
      "extensions": [".hpt"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "hapet",
      "scopeName": "source.hpt",
      "path": "./syntaxes/hapet.tmLanguage.json"
    }]
  }
}
